{
    "parserOptions": {
        "ecmaFeatures":{
            "jsx": true
        }
    },
    "env":{
        "browser"   : true,
        "node"      :true,
        "jquery"    :true
    },
    "rules":{
        "quotes"            : 1,
        "no-trailing-spaces": 0,
    "eol-last"              : 0,
        "no-unused-vars"    : 2,
        "no-undesrcore-dangle": 0,
        "no-alert"          : 0,
        "no-lone-blocks"    : 0
    },
    "globals":{
        "jquery" : true,
        "$"      : true
    }
    
}